</head>

<body>

   <nav class="navbar navbar-default no-margin">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header fixed-brand">
         <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
            <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
         </button>
         <a class="navbar-brand" href="#"><i class="fa fa-dashboard fa-4"></i> Customer </a>
      </div>
      <!-- navbar-header-->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
         <ul class="nav navbar-nav">
            <li class="active">
               <button class="navbar-toggle collapse in" data-toggle="collapse" id="menu-toggle-2"> <span
                     class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
               </button>
            </li>
         </ul>
      </div>
      <!-- bs-example-navbar-collapse-1 -->
   </nav>
   <div id="wrapper">
      <!-- Sidebar -->
      <div id="sidebar-wrapper">
         <ul class="sidebar-nav nav-pills nav-stacked" id="menu">
            <li class="active">
               <a href="/customer-dashboard"><span class="fa-stack fa-lg pull-left"><i
                        class="fa fa-users fa-stack-1x "></i></span>Dashboard</a>
            </li>
            <li>
               <a href="/customer-dashobard/view-servicers"><span class="fa-stack fa-lg pull-left"><i
                        class="fa fa-book fa-stack-1x "></i></span>View Servicers</a>
            </li>
            <li>
               <a href="/logout"><span class="fa-stack fa-lg pull-left"><i
                        class="fa fa-power-off fa-stack-1x "></i></span>Logout</a>
            </li>
         </ul>
      </div>
      <!-- /#sidebar-wrapper -->
      <!-- Page Content -->
      <div id="page-content-wrapper">
         <div class="container-fluid xyz">

            <form class="float-label">

               <div class="form-group">
                  <input type="text" id="date" name="date" class="input-field" disabled>
                  <label for="date" class="float-label">Date:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="username" name="username" class="input-field" disabled>
                  <label for="username" class="float-label">Name:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="companyName" name="companyName" class="input-field" disabled>
                  <label for="companyName" class="float-label">Company name:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="email" name="email" class="input-field" disabled>
                  <label for="email" class="float-label">Email:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="phone" name="phone" class="input-field" disabled>
                  <label for="phone" class="float-label">Phone number:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="customerLocation" name="customerLocation" class="input-field" disabled>
                  <label for="customerLocation" class="float-label">Location:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="serviceLocation" name="serviceLocation" class="input-field" disabled>
                  <label for="serviceLocation" class="float-label">Service Location:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="service" name="service" class="input-field" disabled>
                  <label for="service" class="float-label">Service:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="pickup" name="pickup" class="input-field" disabled>
                  <label for="pickup" class="float-label">Pickup:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="status" name="status" class="input-field" disabled>
                  <label for="status" class="float-label">Status:</label>
               </div>

               <div class="form-group">
                  <input type="text" id="description" name="description" class="input-field" disabled>
                  <label for="description" class="float-label">Description:</label>
               </div>

            </form>
            <div id="map" style="height: 400px; width:100%;"></div>
         </div>
      </div>
   </div>
   <!-- /#page-content-wrapper -->
   </div>
   <!-- /#wrapper -->
   <!-- jQuery -->


   <div class="toasts" id="toasts">
   </div>


</body>

<script src="../../js/Customer/Request Details.js"></script>

<!-- Call the initMap function after the page has loaded -->

<script async defer
   src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBQiPQVE829TRTQxv27e6iBS6m-_fAJJq8&libraries=places&callback=initMap">
   </script>


</html>